{
    "name": "iotse_sample",
    "description": "Sample workflow. Invoke only detector service",
    "version": 1,
    "tasks": [
        {
            "name": "detect",
            "taskReferenceName": "detect",
            "type": "SIMPLE",
            "inputParameters": {
                "iotse_msg": "${workflow.input.iotse_msg}"
                }
        },
        {
            "name": "collect",
            "taskReferenceName": "collect",
            "type": "SIMPLE",
            "inputParameters": {
                "iotse_msg": "${detect.output.iotse_msg}"
                }
        },
        {
            "name": "store",
            "taskReferenceName": "store",
            "type": "SIMPLE",
            "inputParameters": {
                "iotse_msg": "${collect.output.iotse_msg}"
                }
        },
        {
            "name": "search",
            "taskReferenceName": "search",
            "type": "SIMPLE",
            "inputParameters": {
                "iotse_msg": "${workflow.input.iotse_msg}"
                }
        },
        {
            "name": "add_result",
            "taskReferenceName": "add_result",
            "type": "SIMPLE",
            "inputParameters": {
                "iotse_msg": "${search.output.iotse_msg}"
                }
        },
        {
            "name": "get_aggregated_result",
            "taskReferenceName": "get_aggregated_result",
            "type": "SIMPLE",
            "inputParameters": {
                "iotse_msg": "${add_result.output.iotse_msg}"
                }
        }
    ],
    "outputParameters": {
        "iotse_msg": "${get_result.output.iotse_msg}"
        },
    "schemaVersion": 2
}
